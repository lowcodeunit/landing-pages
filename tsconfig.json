{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": "./",
		"outDir": "./dist/out-tsc",
		"sourceMap": true,
		"declaration": false,
		"downlevelIteration": true,
		"experimentalDecorators": true,
		"moduleResolution": "node",
		"importHelpers": true,
		"target": "es2015",
		"module": "es2020",
		"lib": [
			"es2018",
			"dom"
		],
		"paths": {
			"@lowcodeunit/landing-pages-lcu": [
				"dist/lcu-parts/lcu",
				"dist/lcu"
			],
			"@lowcodeunit/landing-pages-lcu-parts": [
				"dist/lcu-parts/lcu-parts",
				"dist/lcu-parts"
			]
		},
		"noImplicitAny": true,
		"suppressImplicitAnyIndexErrors": true
	},
	"angularCompilerOptions": {
		"enableIvy": false,
		"strictInjectionParameters": true,
		"strictTemplates": true
	}
}
